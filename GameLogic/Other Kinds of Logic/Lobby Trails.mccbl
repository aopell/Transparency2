		execute @a[score_sadFaceCount_min=2,score_sadFaceCount=2,tag=equipedTrail_deaths] ~ ~ ~ particle depthsuspend ~ ~0.666 ~ 0.05 0.05 0.05 0 50
		execute @a[score_sadFaceCount_min=2,score_sadFaceCount=2,tag=equipedTrail_deaths] ~ ~ ~ particle depthsuspend ~ ~1.666 ~ 0.05 0.05 0.05 0 50
