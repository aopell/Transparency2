	tellraw @a[score_changeTrail_min=1,tag=hasTrail_parkour2] {"text":"[ Stepping Stone ]","color":"green","clickEvent":{"action":"run_command","value":"/trigger setTrail set 6"},"hoverEvent":{"action":"show_text","value":"Make it to the Lobby Parkour Stone Checkpoint"}}	
	tellraw @a[score_changeTrail_min=1,tag=!hasTrail_parkour2] {"text":"[ Stepping Stone	]","color":"gray","hoverEvent":{"action":"show_text","value":"Make it to the Lobby Parkour Stone Checkpoint"}}	
	tellraw @a[score_changeTrail_min=1,tag=hasTrail_parkour3] {"text":"[ One Small Step ]","color":"green","clickEvent":{"action":"run_command","value":"/trigger setTrail set 7"},"hoverEvent":{"action":"show_text","value":"Make it to the Lobby Parkour Iron Checkpoint"}}	
	tellraw @a[score_changeTrail_min=1,tag=!hasTrail_parkour3] {"text":"[ One Small Step ]","color":"gray","hoverEvent":{"action":"show_text","value":"Make it to the Lobby Parkour Iron Checkpoint"}}	
	tellraw @a[score_changeTrail_min=1,tag=hasTrail_parkour] {"text":"[ Happy Feet ]","color":"green","clickEvent":{"action":"run_command","value":"/trigger setTrail set 1"},"hoverEvent":{"action":"show_text","value":"Complete the Lobby Parkour"}}	
	tellraw @a[score_changeTrail_min=1,tag=!hasTrail_parkour] {"text":"[ Happy Feet ]","color":"gray","hoverEvent":{"action":"show_text","value":"Complete the Lobby Parkour"}}	
	tellraw @a[score_changeTrail_min=1,tag=hasTrail_deaths] {"text":"[ :( ]","color":"green","clickEvent":{"action":"run_command","value":"/trigger setTrail set 2"},"hoverEvent":{"action":"show_text","value":"Die 150 Times"}}
	tellraw @a[score_changeTrail_min=1,tag=!hasTrail_deaths] {"text":"[ :( ]","color":"gray","hoverEvent":{"action":"show_text","value":"Die 150 Times"}}
	tellraw @a[score_changeTrail_min=1,tag=hasTrail_win5] {"text":"[ Victor ]","color":"green","clickEvent":{"action":"run_command","value":"/trigger setTrail set 3"},"hoverEvent":{"action":"show_text","value":"Win 5 Games"}}	
	tellraw @a[score_changeTrail_min=1,tag=!hasTrail_win5] {"text":"[ Victor ]","color":"gray","hoverEvent":{"action":"show_text","value":"Win 5 Games"}}	
	tellraw @a[score_changeTrail_min=1,tag=hasTrail_win15] {"text":"[ Champion ]","color":"green","clickEvent":{"action":"run_command","value":"/trigger setTrail set 4"},"hoverEvent":{"action":"show_text","value":"Win 15 Games"}}	
	tellraw @a[score_changeTrail_min=1,tag=!hasTrail_win15] {"text":"[ Champion ]","color":"gray","hoverEvent":{"action":"show_text","value":"Win 15 Games"}}	
	tellraw @a[score_changeTrail_min=1,tag=hasTrail_god] {"text":"[ Almighty ]","color":"green","clickEvent":{"action":"run_command","value":"/trigger setTrail set 99"},"hoverEvent":{"action":"show_text","value":"Get Every Other Achievement"}}
	tellraw @a[score_changeTrail_min=1,tag=!hasTrail_god] {"text":"[ Almighty ]","color":"gray","hoverEvent":{"action":"show_text","value":"Get Every Other Achievement"}}
	execute @a[score_setTrail_min=1] ~ ~ ~ scoreboard players operation @p @!selectedTrail = @p @!setTrail
	execute @a[score_setTrail=-1] ~ ~ ~ scoreboard players operation @p @!selectedTrail = @p @!setTrail
	//Confirmation Messages
	tellraw @a[score_setTrail_min=-1,score_setTrail=-1] ["",{"text":"You turned off your trail","italic":true,"color":"gold"}]
	tellraw @a[score_setTrail_min=1,score_setTrail=1] ["",{"text":"You equipped the trail","italic":true,"color":"gold"},{"text":" Happy Feet","italic":true,"color":"green"}]
	tellraw @a[score_setTrail_min=2,score_setTrail=2] ["",{"text":"You equipped the trail","italic":true,"color":"gold"},{"text":" :(","italic":true,"color":"green"}]
	tellraw @a[score_setTrail_min=3,score_setTrail=3] ["",{"text":"You equipped the trail","italic":true,"color":"gold"},{"text":" Victor","italic":true,"color":"green"}]
	tellraw @a[score_setTrail_min=4,score_setTrail=4] ["",{"text":"You equipped the trail","italic":true,"color":"gold"},{"text":" Champion","italic":true,"color":"green"}]
	tellraw @a[score_setTrail_min=5,score_setTrail=5] ["",{"text":"You equipped the trail","italic":true,"color":"gold"},{"text":" Baby Steps","italic":true,"color":"green"}]
	tellraw @a[score_setTrail_min=6,score_setTrail=6] ["",{"text":"You equipped the trail","italic":true,"color":"gold"},{"text":" Stepping Stone","italic":true,"color":"green"}]
	tellraw @a[score_setTrail_min=7,score_setTrail=7] ["",{"text":"You equipped the trail","italic":true,"color":"gold"},{"text":" One Small Step","italic":true,"color":"green"}]
	tellraw @a[score_setTrail_min=99,score_setTrail=99] ["",{"text":"You equipped the trail","italic":true,"color":"gold"},{"text":" Almighty","italic":true,"color":"green"}]	
	
	scoreboard players set @a @!setTrail 0
	scoreboard players set @a @!changeTrail 0
