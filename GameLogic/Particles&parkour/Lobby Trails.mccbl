@TYPE REPEAT
@CHAIN ON
@VERSION 1.9
:: "Unlocker"
	scoreboard players set @a @!unlockedTrails 0
	//Parkour 1
	execute @a[score_parkourLevel_min=1,tag=!hasTrail_parkour1] ~ ~ ~ playsound entity.player.levelup @a ~ ~ ~ 1000000 0.7
	execute @a[score_parkourLevel_min=1,tag=!hasTrail_parkour1] ~ ~ ~ tellraw @a ["",{"selector":"@a[score_parkourLevel_min=1,tag=!hasTrail_parkour1]"},{"text":" has just earned the achievement "},{"text":"[ Baby Steps ]","color":"green","hoverEvent":{"action":"show_text","value":"Make it to the Lobby Parkour Wood Checkpoint"}}]
	scoreboard players tag @a[score_parkourLevel_min=1,tag=!hasTrail_parkour1] add @!hasTrail_parkour1
	//Parkour 2
	execute @a[score_parkourLevel_min=2,tag=!hasTrail_parkour2] ~ ~ ~ playsound entity.player.levelup @a ~ ~ ~ 1000000 0.7
	execute @a[score_parkourLevel_min=2,tag=!hasTrail_parkour2] ~ ~ ~ tellraw @a ["",{"selector":"@a[score_parkourLevel_min=2,tag=!hasTrail_parkour2]"},{"text":" has just earned the achievement "},{"text":"[ Stepping Stone ]","color":"green","hoverEvent":{"action":"show_text","value":"Make it to the Lobby Parkour Stone Checkpoint"}}]
	scoreboard players tag @a[score_parkourLevel_min=2,tag=!hasTrail_parkour2] add @!hasTrail_parkour2
	//Parkour 3
	execute @a[score_parkourLevel_min=3,tag=!hasTrail_parkour3] ~ ~ ~ playsound entity.player.levelup @a ~ ~ ~ 1000000 0.7
	execute @a[score_parkourLevel_min=3,tag=!hasTrail_parkour3] ~ ~ ~ tellraw @a ["",{"selector":"@a[score_parkourLevel_min=3,tag=!hasTrail_parkour3]"},{"text":" has just earned the achievement "},{"text":"[ One Small Step ]","color":"green","hoverEvent":{"action":"show_text","value":"Make it to the Lobby Parkour Iron Checkpoint"}}]
	scoreboard players tag @a[score_parkourLevel_min=3,tag=!hasTrail_parkour3] add @!hasTrail_parkour3
	//Parkour Finish
	execute @a[score_parkourLevel_min=12,tag=!hasTrail_parkour] ~ ~ ~ playsound entity.player.levelup @a ~ ~ ~ 1000000 0.7
	execute @a[score_parkourLevel_min=12,tag=!hasTrail_parkour] ~ ~ ~ tellraw @a ["",{"selector":"@a[score_parkourLevel_min=12,tag=!hasTrail_parkour]"},{"text":" has just earned the achievement "},{"text":"[ Happy Feet ]","color":"green","hoverEvent":{"action":"show_text","value":"Complete the Lobby Parkour"}}]
	scoreboard players tag @a[score_parkourLevel_min=12,tag=!hasTrail_parkour] add @!hasTrail_parkour
	//Deaths
	execute @a[score_deathsAch_min=150,tag=!hasTrail_deaths] ~ ~ ~ playsound entity.player.levelup @a ~ ~ ~ 1000000 0.7
	execute @a[score_deathsAch_min=150,tag=!hasTrail_deaths] ~ ~ ~ tellraw @a ["",{"selector":"@a[score_deathsAch_min=150,tag=!hasTrail_deaths]"},{"text":" has just earned the achievement "},{"text":"[ :( ]","color":"green","hoverEvent":{"action":"show_text","value":"Die 150 Times"}}]
	scoreboard players tag @a[score_deathsAch_min=150,tag=!hasTrail_deaths] add @!hasTrail_deaths
	//5 wins
	execute @a[score_wins_min=5,tag=!hasTrail_win5] ~ ~ ~ playsound entity.player.levelup @a ~ ~ ~ 1000000 0.7
	execute @a[score_wins_min=5,tag=!hasTrail_win5] ~ ~ ~ tellraw @a ["",{"selector":"@a[score_wins_min=5,tag=!hasTrail_win5]"},{"text":" has just earned the achievement "},{"text":"[ Victor ]","color":"green","hoverEvent":{"action":"show_text","value":"Win 5 Games"}}]
	scoreboard players tag @a[score_wins_min=5,tag=!hasTrail_win5] add @!hasTrail_win5
	//15 wins
	execute @a[score_wins_min=15,tag=!hasTrail_win15] ~ ~ ~ playsound entity.player.levelup @a ~ ~ ~ 1000000 0.7
	execute @a[score_wins_min=15,tag=!hasTrail_win15] ~ ~ ~ tellraw @a ["",{"selector":"@a[score_wins_min=15,tag=!hasTrail_win15]"},{"text":" has just earned the achievement "},{"text":"[ Champion ]","color":"green","hoverEvent":{"action":"show_text","value":"Win 15 Games"}}]
	scoreboard players tag @a[score_wins_min=15] add @!hasTrail_win15
	//final
	scoreboard players add @a[tag=hasTrail_parkour] @!unlockedTrails 1
	scoreboard players add @a[tag=hasTrail_deaths] @!unlockedTrails 1
	scoreboard players add @a[tag=hasTrail_win5] @!unlockedTrails 1
	scoreboard players add @a[tag=hasTrail_win15] @!unlockedTrails 1
	scoreboard players tag @a[score_unlockedTrails_min=4] add @!hasTrail_god
;;
